{
  "name": "api",
  "version": "1.0.0",
  "description": "Aster Volume Trading Bot API Service",
  "main": "dist/infrastructure/api/main.js",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/infrastructure/api/main.js",
    "dev": "ts-node -r tsconfig-paths/register src/infrastructure/api/main.ts",
    "watch": "nodemon --exec ts-node -r tsconfig-paths/register src/infrastructure/api/main.ts",
    "test": "jest",
    "clean": "rm -rf dist node_modules logs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "auth:demo": "ts-node -r tsconfig-paths/register src/infrastructure/auth/demo.ts",
    "debug:prompt": "ts-node -r tsconfig-paths/register src/scripts/debug/test-prompt-generator.ts"
  },
  "keywords": [
    "aster",
    "dex",
    "futures",
    "volume",
    "farming",
    "bot",
    "trading",
    "api"
  ],
  "author": "Volume Farming Team",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.2",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.6.2",
    "bs58": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.5",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0",
    "postgres": "^3.4.7",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.0",
    "web3": "^4.16.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
